        <table id="dtBasicExample" class="table table-striped table-bordered table-responsive-sm" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th class="th-sm">IP
                    </th>
                    <th class="th-sm">MAC
                    </th>
                    <th class="th-sm">Reseau
                    </th>
                    <th class="th-sm">OS
                    </th>
                    <th class="th-sm">Statut
                    </th>
                    <th class="th-sm">Employ√©
                    </th>
                </tr>
            </thead>

            <tbody>
                {% for o in listeO %}
                    <tr>
                        <td>{{o.ip}}</td>
                        <td>{{o.mac}}</td>
                        <td>{{o.reseau}}</td>
                        <td>{{o.nomOs}}</td>
                        <td>{{o.nomStatut}}</td>
                        <td>{{o.unEmploye}}</td>
                    </tr>
                {% endfor %}
        </table>

    </div>


{% endblock %}